<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.activequant.persistence.TradeStatistics" table="TradeStatistics">
    <id name="id" column="ID" type="long">
      <generator class="native"/>
    </id>

		<property name="tradeId" />
		<property name="maxPnl" />
		<property name="minPnl" />
		<property name="timeStampMs" />
		<property name="description" />
		<property name="lastPnl" />
		<property name="lastQuoteTimeStampMs" />
		<property name="sessionId" />
		
	</class>
</hibernate-mapping>


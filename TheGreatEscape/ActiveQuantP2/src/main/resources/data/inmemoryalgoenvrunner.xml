<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>

    <bean id="exporter" class="org.springframework.jmx.export.MBeanExporter">
        <property name="beans">
            <map>
                <entry key="bean:name=runner" value-ref="runner"/>
            </map>
        </property>
        <property name="registrationBehaviorName" value="REGISTRATION_REPLACE_EXISTING"/>
    </bean>
    
    <bean id="jmsConnection" class="org.activequant.util.tempjms.JMS">
    	
	    <constructor-arg>	    	
	    		<value>192.168.0.103</value>	  	
	    </constructor-arg>
	     <constructor-arg>
	      		<value>7676</value>	
	    </constructor-arg>
    </bean>

	<bean id="ctsBroker" class="org.activequant.util.cts.CTSBroker">
    	
	    <constructor-arg>	    	
	    		<value>192.168.0.26</value>	  	
	    </constructor-arg>
	     <constructor-arg>
	      		<value>22225</value>	
	    </constructor-arg>
	    <constructor-arg>
	    	<value>Sandy</value>
	    </constructor-arg>
    </bean>



    <bean id="runner" class="org.activequant.production.InMemoryAlgoEnvConfigRunner">	   
	    <constructor-arg>
	    	<ref bean="jmsConnection"/>
	    </constructor-arg>
	    <constructor-arg>
	    <ref bean="ctsBroker"/>
	    </constructor-arg>
    </bean>

</beans>